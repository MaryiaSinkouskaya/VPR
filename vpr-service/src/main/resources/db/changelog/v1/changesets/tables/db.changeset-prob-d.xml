<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="create-prob-d-table" author="maryia">
        <comment>Create prob_d table with sequence</comment>

        <createSequence
                sequenceName="prob_d_id_seq"
                startValue="20"
                incrementBy="1"/>

        <createTable tableName="prob_d">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="death_date" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="proband_id" type="INTEGER"/>
        </createTable>

    </changeSet>

</databaseChangeLog>
